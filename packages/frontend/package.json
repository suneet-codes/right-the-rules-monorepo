{
  "name": "@right-the-rules/frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "yarn eslint --fix \"./**/*.{js,jsx,ts,tsx,json}\"",
    "format": "yarn prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .gitignore",
    "pre-commit": "lint-staged",
    "pre-push": "yarn jest --coverage"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.4.0",
    "lint-staged": "^11.1.1",
    "next": "11.0.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "jest-dom": "^4.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
